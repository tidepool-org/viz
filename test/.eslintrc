{
  "env": {
    "mocha": true,
    "node": true
  },
  "globals": {
    "assert": false,
    "expect": false,
    "sinon": false
  },
  "plugins": [
    "moment-utc"
  ],
  "rules": {
    "lodash/prefer-lodash-method": ["error", {"except": ["find"]}], // because of Enzyme .find
    "moment-utc/no-moment-without-utc": 2,
    "no-unused-expressions": "off"
  }
}